{
  "name": "fenixcorp/api",
  "description": "FenixCorp multi-tenant POS API",
  "type": "project",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "smalot/pdfparser": "^0.16.1",
    "spatie/laravel-permission": "^6.0",
    "picqer/php-barcode-generator": "^2.2",
    "dompdf/dompdf": "^2.0",
    "maatwebsite/excel": "^3.1",
    "laravel/sanctum": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
    ],
    "start": [
      "php artisan serve"
    ]
  }
}
